{
  "kind": "collectionType",
  "collectionName": "order_items",
  "info": {
    "singularName": "order-item",
    "pluralName": "order-items",
    "displayName": "order_item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "amount": {
      "type": "decimal",
      "min": 0
    },
    "externalid": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "string"
    },
    "sku": {
      "type": "string"
    },
    "payment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::payment.payment",
      "inversedBy": "products"
    }
  }
}
